{
    "name": "armincms/categorizable",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "armincms"
    ],
    "license": "MIT",
    "require": { 
        "armincms/markable": "~0.1", 
        "armincms/taggable": "~2.0", 
        "kalnoy/nestedset": "~6.0",
        "whitecube/nova-flexible-content": "~0.1",
        "zareismail/complex": "~0.3"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\Categorizable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [ 
                "Armincms\\Categorizable\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "2.0.0"
}
